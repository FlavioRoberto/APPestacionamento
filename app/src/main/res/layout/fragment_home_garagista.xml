<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.appestacionamento.cursoandroid.admin.appestacionamento.Activity.Fragments.FragmentGaragista.HomeGaragista">

    <!-- TODO: Update blank fragment layout -->

    <ListView
        android:id="@+id/ListviewId"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:padding="20dp"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:layout_marginTop="270dp"
        android:orientation="vertical">

        <SeekBar
            android:id="@+id/seekBarSetores"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="20dp" />

        <TextView
            android:id="@+id/Setor"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/setor1"
            android:textAlignment="center" />

        <Button
            android:id="@+id/BuscarSetor"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:background="@drawable/ovalbutton"
            android:text="@string/Buscar" />
    </LinearLayout>
</FrameLayout>
