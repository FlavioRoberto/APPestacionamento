<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.appestacionamento.cursoandroid.admin.appestacionamento.Activity.Fragments.FragmentGaragista.HomeGaragista"
    android:background="#fff">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/pesquisaId"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:background="@drawable/ovaltext_busca"
            app:srcCompat="@drawable/ic_search" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Pesquisar Vagas"
            android:textSize="@dimen/TextSize"
            android:textColor="#000"
            android:textAlignment="center"/>
    </LinearLayout>

</FrameLayout>
