<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appestacionamento.cursoandroid.admin.appestacionamento">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />

        <activity android:name=".Activity.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Activity.LoginActivity" />
        <activity android:name=".Activity.Activity.ConsultaUsuarioActivity" />
        <activity android:name=".Activity.Activity.AdmActivity" />
        <activity android:name=".Activity.Activity.CadastroUsuarioActivity" />
        <activity android:name=".Activity.Activity.EditaDadosActivity" />
        <activity android:name=".Activity.Activity.CadastroVeicuoActivity" />
        <activity android:name=".Activity.Activity.ExcluirVeiculoActivity"></activity>
    </application>

</manifest>